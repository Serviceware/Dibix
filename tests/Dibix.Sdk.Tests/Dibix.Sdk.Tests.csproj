<Project Sdk="Microsoft.NET.Sdk">
  
  <Import Project="..\..\build\common.props" />

  <PropertyGroup>    
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>Dibix.Sdk.Tests</AssemblyName>
    <VersionPrefix>2.0.0</VersionPrefix>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Update="LintTest.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>LintTest.cs</LastGenOutput>
    </None>
    <None Update="ParserTest.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>ParserTest.cs</LastGenOutput>
    </None>
    <EmbeddedResource Update="Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <None Update="SourcesTest.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>SourcesTest.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="LintTest.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>LintTest.tt</DependentUpon>
    </Compile>
    <Compile Update="ParserTest.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ParserTest.tt</DependentUpon>
    </Compile>
    <Compile Update="Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
    <Compile Update="SourcesTest.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SourcesTest.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="$(MicrosoftTestSdkVersion)" />
    <PackageReference Include="xunit" Version="$(XUnitVersion)" />
    <PackageReference Include="xunit.runner.visualstudio" Version="$(XUnitVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\Dibix\Dibix.csproj" />
    <ProjectReference Include="..\..\src\Dibix.Sdk\Dibix.Sdk.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>

</Project>
