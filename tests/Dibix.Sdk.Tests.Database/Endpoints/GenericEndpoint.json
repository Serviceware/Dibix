{
  "$schema": "../../../src/Dibix.Sdk/CodeGeneration/Schema/dibix.endpoints.schema.json",
  "GenericEndpoint": [
    //"Dibix.GenericEndpoint",
    {
      "method": "GET",
      "target": "MultiConcreteResult"
    },
    {
      "method": "GET",
      "target": "EmptyWithParams",
      "childRoute": "{password}/Fixed",
      "params": {
        "a": "HEADER.User-Agent",
        "b": "HEADER.Authorization.Parameter",
        "c": {
          "source": "DBX.X",
          "converter": "DBX"
        },
        "password": null,
        "d": "REQUEST.Language",
        "e": true,
        "f": "Descending",
        "g": "cake"
      }
    },
    {
      "method": "GET",
      "target": "EmptyWithParams",
      "childRoute": "{password}/User",
      "authorization": "Anonymous",
      "operationId": "EmptyWithParamsAnonymous"
    },
    {
      "method": "GET",
      "target": "SingleConrecteResultWithParams",
      "childRoute": "Array"
    },
    {
      "method": "GET",
      "target": "FileResult",
      "childRoute": "{id}",
      "authorization": [
        "Anonymous",
        "Bearer"
      ],
      "fileResponse": {
        "mediaType": "image/*",
        "cache": false
      }
    },
    {
      "method": "PUT",
      "target": "FileUpload",
      "body": {
        "mediaType": "image/png"
      },
      "params": {
        "data": "BODY.$RAW"
      }
    },
    {
      "method": "POST",
      "target": "EmptyWithParams",
      "body": "InputContract",
      "params": {
        "ids": {
          "convertFromBody": "Dibix.GenericContractIdsInputConverter"
        }
      }
    },
    {
      "method": "PATCH",
      "target": "EmptyWithParams",
      "body": "InputContract"
    },
    {
      "method": "DELETE",
      "target": "EmptyWithParams",
      "body": "AnotherInputContract",
      "params": {
        "ids": {
          "source": "BODY.SomeIds",
          "items": {
            "id": 1,
            "name": "ITEM.Title"
          }
        }
      },
      "authorization": {
        "target": "AssertAuthorized",
        "params": {
          "right": 1
        }
      }
    }
  ]
}