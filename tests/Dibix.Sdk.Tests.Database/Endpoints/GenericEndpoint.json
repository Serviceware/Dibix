{
  "$schema": "../../../src/Dibix.Sdk/CodeGeneration/Schema/dibix.endpoints.schema.json",
  "GenericEndpoint": [
    "Dibix.GenericEndpoint",
    {
      "method": "GET",
      "target": "EmptyWithParams",
      "childRoute": "{password}/Fixed",
      "params": {
        "v": "HEADER.Authorization.Scheme",
        "w": "HEADER.Authorization.Parameter",
        "x": "DBX.X",
        "password": null,
        "y": true,
        "z": 5
      }
    },
    {
      "method": "GET",
      "target": "EmptyWithParams",
      "childRoute": "{password}/User",
      "isAnonymous": true
    },
    {
      "method": "GET",
      "target": "SingleConrecteResultWithParams",
      "childRoute": "Array"
    },
    {
      "method": "GET",
      "target": "FileResult",
      "childRoute": "{id}",
      "fileResponse": {
        "mediaType": "image/*",
        "cache": false 
      }
    },
    {
      "method": "PUT",
      "target": "FileUpload",
      "body": {
        "mediaType": "image/png"
      },
      "params": {
        "data": "BODY.$RAW"
      } 
    },
    {
      "method": "POST",
      "target": "EmptyWithParams",
      "body": "InputContract",
      "params": {
        "ids": {
          "convertFromBody": "Dibix.GenericContractIdsInputConverter"
        }
      }
    }
  ]
}