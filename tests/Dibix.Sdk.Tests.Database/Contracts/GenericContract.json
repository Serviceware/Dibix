{
  "Role": [
    "None",
    "User",
    "Admin"
  ],
  "GenericContract": {
    "$wcfNs": "https://schemas.dibix.com/GenericContract",
    "Id": {
      "type": "int32",
      "isPartOfKey": true,
      "serialize": "Never"
    },
    "Name": {
      "type": "string",
      "serialize": "IfNotEmpty",
      "default": "DefaultValue"
    },
    "ParentId": {
      "type": "int32?",
      "isDiscriminator": true
    },
    "Role": {
      "type": "#Role",
      "default": "User"
    },
    "CreationTime": {
      "type": "datetime?",
      "kind": "utc"
    }
  }
}