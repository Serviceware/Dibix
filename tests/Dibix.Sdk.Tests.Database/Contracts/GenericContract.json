{
  "GenericContract": {
    "$wcfNs": "https://schemas.dibix.com/GenericContract",
    "Id": {
      "type": "int32",
      "isPartOfKey": true,
      "serialize": "Never"
    },
    "Name": {
      "type": "string",
      "serialize": "IfNotEmpty",
      "default": "DefaultValue"
    },
    "ParentId": {
      "type": "int32?",
      "isDiscriminator": true
    },
    "Role": {
      "type": "Role",
      "default": "User",
      "enumFormat": "String"
    },
    "CreationTime": {
      "type": "datetime?",
      "kind": "utc",
      "serialize": "IfNotEmpty"
    },
    "ImageUrl": {
      "type": "uri",
      "isRelativeHttpsUrl": true
    },
    "TheDate": "date",
    "TheTime": "time"
  },
  "Role": [
    "None",
    "User",
    "Admin"
  ]
}