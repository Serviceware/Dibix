{
  "Endpoints": {
    "BaseUrl": "https://localhost/api",
    "ParameterSources": {
      "DBX": {
        "X": "uuid",
        "Y": {
          "type": "int32",
          "requiredClaims": [
            "Name"
          ]
        }
      },
      "CLAIM": {
        "Id": {
          "type": "string",
          "claimName": "sub"
        },
        "Name": "string"
      }
    },
    "Converters": [
      {
        "name": "DBX",
        "requiredClaims": [
          "Name"
        ]
      },
      "DBY"
    ],
    "CustomSecuritySchemes": {
      "DibixClientId": {
        "type": "Header",
        "headerName": "DBXNS-ClientId"
      },
      "DibixBearer": "Bearer"
    },
    "Templates": {
      "Default": {
        "Action": {
          "securitySchemes": "DibixBearer"
        }
      },
      "Authorization": {
        "AssertAuthorized": {
          "target": "AssertAuthorized"
        },
        "AssertAuthorizedOne": {
          "target": "AssertAuthorized",
          "params": {
            "right": 1
          }
        }
      }
    }
  }
}