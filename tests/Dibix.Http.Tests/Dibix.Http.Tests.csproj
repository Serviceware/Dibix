    <PackageReference Include="xunit.runner.visualstudio" Version="$(XUnitVSVersion)" />
    <PackageReference Include="coverlet.msbuild" Version="$(CoverletVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\Dibix.Http\Dibix.Http.csproj" />
  </ItemGroup>

