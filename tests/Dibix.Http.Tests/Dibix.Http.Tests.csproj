<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>    
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>Dibix.Http.Tests</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="Dibix.Http.ApiRegistrationAttribute">
      <_Parameter1>Dibix</_Parameter1>
    </AssemblyAttribute>
